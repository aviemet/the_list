{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["deno.window"],
		"module": "esnext",
		"moduleResolution": "bundler",
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noImplicitReturns": true,
		"skipLibCheck": true,
		"allowJs": true,
		"checkJs": false,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"types": ["https://deno.land/x/types/index.d.ts"]
	}
}

